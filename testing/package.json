{
  "name": "webpack-es6",
  "version": "1.0.0",
  "description": "starter kit for webpack-es6",
  "main": "app.js",
  "scripts": {
    "start": "node --use_strict server.js",
    "go": "npm-run-all --parallel dev-server dev-frontend",
    "dev-server": "dotnet run",
    "dev-frontend": "webpack --config webpack.config.dev.js --watch",
    "build-dist": "npm-run-all --parallel compile-project compile-static-assets",
    "compile-static-assets": "webpack",
    "compile-project": "dotnet restore"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-preset-stage-0": "^6.24.1",
    "google-map-react": "^0.25.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-motion": "^0.5.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-transition": "^1.1.1",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-core": "6.22.1",
    "babel-loader": "6.2.10",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-beta.5",
    "file-loader": "^0.10.1",
    "node-sass": "4.5.0",
    "nodemon": "1.11.0",
    "npm-run-all": "4.0.1",
    "resolve-url-loader": "^2.0.0",
    "sass-loader": "5.0.1",
    "style-loader": "0.13.1",
    "webpack": "2.2.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-middleware": "1.5.1"
  }
}
